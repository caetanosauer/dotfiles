Host bitbucket
    HostName bitbucket.org
    PreferredAuthentications publickey
    IdentityFile /home/csauer/.ssh/id_bitbucket
    IdentitiesOnly yes

Host ds1
    HostName ds-server1
    PreferredAuthentications publickey
    # IdentityFile /home/csauer/.ssh/id_lgis
    IdentitiesOnly yes

Host ds2
    HostName ds-server2
    PreferredAuthentications publickey
    # IdentityFile /home/csauer/.ssh/id_lgis
    IdentitiesOnly yes

# DS1 with tunnel in localhost
Host ds1r
    HostName localhost
    Port 2100
    # IdentityFile /home/csauer/.ssh/id_lgis

# DS2 with tunnel in localhost
Host ds2r
    HostName localhost
    Port 2200
    # IdentityFile /home/csauer/.ssh/id_lgis

Host lts1
    HostName lts1.informatik.uni-kl.de
    LocalForward 2200 ds-server2:22
    LocalForward 2100 ds-server1:22
    DynamicForward 3128
    # PreferredAuthentications publickey
    # IdentityFile /home/csauer/.ssh/id_lgis
    # IdentitiesOnly yes

